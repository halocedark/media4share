{
  "name": "Media4Share",
  "version": "0.1.0",
  "description": "Sharing files with others made easy",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "publish": "electron-builder -p always"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/halocedark/media4share-updates.git"
  },
  "build": {
    "publish": [{
       "provider": "github",
       "owner": "halocedark",
       "repo": "media4share-updates"
    }],
    "appId": "com.holoolaz.media4share",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ejs-electron": "^2.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-root-path": "^1.0.16",
    "electron-updater": "^4.6.5",
    "express": "^4.17.3",
    "ini": "^2.0.0",
    "jquery": "^3.6.0",
    "js-sha1": "^0.6.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13"
  }
}
